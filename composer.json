{
  "name": "isvek/theme",
  "type": "wordpress-theme",
  "description": "Isvek Theme",
  "version": "1.0.5",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Veks",
      "email": "oleg@isvek.ru",
      "homepage": "https://isvek.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "rappasoft/laravel-helpers": "^v2.1.0"
  },
  "autoload": {
    "psr-4": {
      "Isvek\\Theme\\": "inc"
    },
    "files": [
      "inc/functions.php",
      "inc/template-hooks.php",
      "inc/template-functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "stable"
}
